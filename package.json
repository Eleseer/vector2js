{
  "name": "@aliser/vector2",
  "version": "1.0.1",
  "description": "Some basic functionality to represent and work with 2D vectors.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run readme && npm run compile",
    "compile": "rimraf dist && tsc",
    "readme": "rimraf readme.md && typedoc --plugin typedoc-plugin-markdown src/index.ts --disableSources && concat-md --toc --decrease-title-levels --dir-name-as-title docs > README.md && rimraf docs",
    "test": "ts-mocha -p tsconfig.test.json test/*.*",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "math",
    "vector",
    "2D"
  ],
  "author": "@aliser",
  "license": "MIT",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.0",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "concat-md": "^0.3.5",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.19.0",
    "minify": "^7.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/Eleseer/vector2js.git"
  },
  "bugs": {
    "url": "https://github.com/Eleseer/vector2js/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module"
}
